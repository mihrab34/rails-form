<%= form_for @user do |f| %>
   <%= f.label :username %><br>
   <%= f.text_field :username, placeholder: "Enter Username"  %><br>
   <%= f.label :email %><br>
   <%= f.email_field :email, placeholder: "example@example.com"  %><br>
   <%= f.label :password %><br>
   <%= f.password_field :password, placeholder: "Enter Password"  %><br>
   <%= f.submit "Submit"%>
<% end %>